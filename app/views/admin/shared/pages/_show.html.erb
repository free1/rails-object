<h3 class="page-header">详情</h3>
<%= render "links" rescue render "admin/shared/links" %>
<br/><br/>
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <%=raw title %>
      </div>
      <div class="panel-body">
        <div class="dataTable_wrapper">
          <%= yield :native_header %>
          <dl>
            <% display_arr.each do |elem| %>
              <dt>
                <span class="label label-primary"><%= i18n_attribute_name_by_resource(resource, elem) %>：</span>

              </dt>
              <dd>
                <%= render :partial => "admin/shared/pages/record", :locals => {:resource => resource, :elem => elem} %>
              </dd>
            <% end %>
          </dl>

          <%= yield :extra_body %>
        </div>
      </div>
    </div>
  </div>
</div>

<%= yield :extra_content %>
